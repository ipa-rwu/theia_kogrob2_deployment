grammar de.fraunhofer.ipa.deployment.Deployment with org.eclipse.xtext.common.Terminals

generate deployment "http://www.fraunhofer.de/ipa/deployment/Deployment"

StateMachine:
	'statemachine' name=ID
	(states+=State | events+=Event)*;

State:
	'state' name=ID transitions+=Transition*;

Event:
	'event' name=ID;

Transition:
	event=[Event] '=>' state=[State];
